# Application Settings
APP_NAME=Junjo Server
DEBUG=true
HOST=0.0.0.0
PORT=1324

# gRPC Server (Internal Authentication Service)
# This port is used by ingestion-service to validate API keys
# WARNING: This port should ONLY be accessible on internal Docker network in production
GRPC_PORT=50053

# CORS (JSON array format or comma-separated)
CORS_ORIGINS=["http://localhost:5151"]

# Database
DB_SQLITE_PATH=./dbdata/junjo.db
DB_DUCKDB_PATH=./dbdata/traces.duckdb

# Ingestion Service (Go gRPC service)
INGESTION_HOST=localhost
INGESTION_PORT=50052

# Session Cookie Security Settings
# Generate secure keys with: openssl rand -base64 32
# IMPORTANT: Use different keys in production! Never commit real keys to version control!
JUNJO_SECURE_COOKIE_KEY=REPLACE_WITH_OUTPUT_FROM_openssl_rand_base64_32
JUNJO_SESSION_SECRET=REPLACE_WITH_OUTPUT_FROM_openssl_rand_base64_32
JUNJO_ENV=development
JUNJO_PROD_AUTH_DOMAIN=
